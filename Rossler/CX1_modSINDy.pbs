#!/bin/bash
#PBS -J 1-176
#PBS -l select=1:ncpus=4:ompthreads=4:mem=80gb
#PBS -l walltime=8:00:00
#PBS -N Rossler_modSINDy
#PBS -k oed

module load tools/prod
module load TensorFlow/2.11.0-foss-2022a-CUDA-11.7.0

cd $PBS_O_WORKDIR

# Presuming all run scripts are inside $PBS_O_WORKDIR
# (the directory where the qsub command is called)
# copy all the script from there to the temporary directory
for i in $(seq 1 20);
do
while [ ! -f "./Result/${PBS_ARRAY_INDEX}_${i}_NSS_SINDy_Rossler.mat" ] 
do
python NSS_SINDy_Approach_SingleRun_Rossler.py $PBS_ARRAY_INDEX $i
done
done

exit 0
